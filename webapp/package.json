{
  "name": "retirement-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:test": "E2E_TEST_MODE=true NEXT_PUBLIC_E2E_TEST_MODE=true next dev",
    "build": "prisma generate && next build",
    "analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "test": "npx tsx test-calculations.ts",
    "test:watch": "npx tsx watch test-calculations.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:strategies": "playwright test e2e/simulation-strategies.spec.ts",
    "test:e2e:edge-cases": "playwright test e2e/simulation-edge-cases.spec.ts",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "analytics": "npx tsx scripts/user-analytics.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@marsidev/react-turnstile": "^1.4.0",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.29.0",
    "@stripe/stripe-js": "^8.6.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.6",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.14.0",
    "jose": "^6.1.1",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.554.0",
    "next": "^15.5.11",
    "next-auth": "^4.24.13",
    "pg": "^8.16.3",
    "prisma": "^6.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.66.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.5.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.6.0",
    "stripe": "^20.2.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.0",
    "@playwright/test": "^1.57.0",
    "@types/html2pdf.js": "^0.10.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.22",
    "eslint": "^8",
    "eslint-config-next": "^15.5.9",
    "playwright": "^1.57.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vercel": "^49.2.0"
  }
