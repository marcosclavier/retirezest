# FastAPI Backend Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=8000
PYTHON_ENV=development

# CORS Origins (comma-separated)
# In production, set to your Vercel domain only
ALLOWED_ORIGINS=http://localhost:3002,http://localhost:3000,https://your-app.vercel.app

# Logging
LOG_LEVEL=INFO

# Optional: Database (if you add persistence later)
# DATABASE_URL=postgresql://user:password@localhost:5432/retirement_db
